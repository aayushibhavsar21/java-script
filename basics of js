      } 

   //2.array
  
   var arr=[1,2,3,4,5,6,7];
   console.log(arr);
   console.log(arr[0]);
   console.log(arr[4]);

   var arr2=['good','morning','friends'];
   console.log(arr2);
   console.log(arr2[2]);

   //___loop____ 
  
    for(var i=0;i<arr.length;i++)
    {
      if(i==5)
        { break;}
      console.log(arr[i]);
    }

    let j=0;

    while(j<arr2.length)
    {
      if(j==0)
      { continue; }
      console.log(arr2[j]);
      j++;
    }
 
    let k=0;
    do
    {
      console.log(arr2[k]);
      k++;
    } 
    while(k<arr2.length)
    
