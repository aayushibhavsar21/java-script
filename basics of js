    //_____primitive data types_____
    //1.numbers

    var n1=34;
    var n2=67;
    console.log(n1+n2);
    document.write("\n",n1+n2);

    //2.string 

    var str1="good";
    var str2="morning";

    //3.boolean

   var a=true;
   var b=false;
   console.log(a,b);
  
   //4.undefined 
  
   var c;
   console.log(c);
 
   //5.null 

    var ab=null;
    console.log(ab);

   //_____reference data type_____
  
   
